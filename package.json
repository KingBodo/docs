  "devDependencies": {
    "markdown-link-check": "^3.8.1",
    "markdownlint-cli": "^0.23.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
